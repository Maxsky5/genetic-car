{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "member-ordering": false,
        "max-line-length": [
            true,
            140
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}
